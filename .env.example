# Hyperliquid direct API (defaults to mainnet if API base not set)
HYPERLIQUID_NETWORK=mainnet
# Optional override; otherwise we use https://api.hyperliquid.xyz or the testnet URL.
# HYPERLIQUID_API_BASE=https://api.hyperliquid.xyz
HYPERLIQUID_WALLET_ADDRESS=0xYourAccountAddress
HYPERLIQUID_PRIVATE_KEY=0xyoursecretkey
# Set only if you need to target a builder / custom dex name.
# HYPERLIQUID_DEX=

# Strategy sizing
HYPERLIQUID_PORTFOLIO_VALUE=997.5
HYPERLIQUID_MIN_ORDER_VALUE=10
HYPERLIQUID_SYMBOLS=BTC-PERP,ETH-PERP,SOL-PERP

# Agent Market integration
AGENT_MARKET_API_KEY=replace-with-agent-market-key
AGENT_MARKET_MAX_CREDIT=0.05
AGENT_MARKET_INSTANCE_TIMEOUT=90
AGENT_MARKET_POLL_INTERVAL=5
AGENT_MARKET_MAX_POLLS=18
